{
  "name": "angelblanco-web",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.18.0+sha512.e804f889f1cecc40d572db084eec3e4881739f8dec69c0ff10d2d1beff9a4e309383ba27b5b750059d7f4c149535b6cd0d2cb1ed3aeb739239a4284a68f40cfa",
  "engines": {
    "node": ">=v22.14.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint --parallel",
    "lint:fix": "turbo run lint:fix --parallel",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "cleanup": "turbo run cleanup",
    "prepare": "husky",
    "preview": "pnpm build && npx serve apps/angelblanco.dev/.output/public",
    "fresh": "bash ./fresh.sh",
    "ai:flash": "gemini -m gemini-2.5-flash",
    "ai:commit": "pnpm ai:flash --prompt-interactive \"/git:commit\""
  },
  "devDependencies": {
    "@angelblanco/eslint": "workspace:*",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^22.16.5",
    "eslint": "^9.37.0",
    "husky": "^9.1.7",
    "turbo": "^2.5.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "sharp"
    ]
  }
}
