{
  "name": "angelblanco-web",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd",
  "engines": {
    "node": ">=v24.10.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint --parallel",
    "lint:fix": "turbo run lint:fix --parallel",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "cleanup": "turbo run cleanup",
    "prepare": "husky",
    "preview": "pnpm build && npx serve apps/angelblanco.dev/.output/public",
    "fresh": "bash ./fresh.sh",
    "ai:flash": "gemini -m gemini-2.5-flash",
    "ai:commit": "pnpm ai:flash --prompt-interactive \"/git:commit\""
  },
  "devDependencies": {
    "@angelblanco/eslint": "workspace:*",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^24.7.1",
    "eslint": "^9.37.0",
    "husky": "^9.1.7",
    "turbo": "^2.5.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "sharp"
    ]
  }
}
